{
open Dyp
exception Syntax_Error of Lexing.position * Language.syntax_error
}

%start main

%lexer

let space = [' ' '\t' '\r']
let newline = '\n'
let contents = [^'\n']+

main lexer =
  newline space* newline -> PARSKIP

%parser

paragraph:
| contents<c> {c}

paragraphs:
| {[]}
| paragraph<t> newline? paragraphs<q> { q }	
| paragraph<t> PARSKIP paragraphs<q> { t::q }	

main: 
 | {[]}
 | newline? paragraphs<result> PARSKIP? eof { result }

